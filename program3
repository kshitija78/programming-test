#include <stdio.h>
void main()
{
   int i,n;

   printf("Input number of terms : ");
   scanf("%d",&n);
   if(n%2!=0){
   printf("\nThe odd numbers are :");
   for(i=1;i<=n;i++)
   {
       
     printf("%d ",2*i-1);
   }
   
   }else{
       if(n%2==0)
       printf("\nThe odd numbers are :");
   for(i=1;i<=n-1;i++)
   {
       
     printf("%d ",2*i-1);
   }
   
   }
}
